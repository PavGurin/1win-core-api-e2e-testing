{
  "name": "node_starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "jest": {
    "testRegex": "./test/.*.[js]?$",
    "transform": {
      ".*": "./node_modules/babel-jest"
    },
    "setupTestFrameworkScriptFile": "./test/global.js",
    "reporters": [
      "default",
      "./src/reporters/jest-testrail.js"
    ]
  },
  "scripts": {
    "test": "jest --testTimeout=10000 --colors --reporters=\"default\" --reporters=\"./src/reporters/jest-testrail.js\" --maxWorkers=4",
    "eslint": "eslint ./ --ext .js",
    "eslint:fix": "eslint ./ --ext .js --fix",
    "test:banking": "jest --testPathPattern=./test/banking/.* --testTimeout=10000 --reporters=\"default\" --reporters=\"./src/reporters/jest-testrail.js\" --maxWorkers=4",
    "test:bets": "jest --testPathPattern=./test/bets/.* --testTimeout=10000 --reporters=\"default\" --reporters=\"./src/reporters/jest-testrail.js\" --maxWorkers=4",
    "test:health": "jest --testPathPattern=./test/health/.* --testTimeout=10000 --reporters=\"default\" --maxWorkers=4",
    "test:user-profile": "jest --testPathPattern=./test/userProfile/.* --testTimeout=10000 --reporters=\"default\" --reporters=\"./src/reporters/jest-testrail.js\" --maxWorkers=4"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.4",
    "@babel/register": "^7.0.0",
    "babel-node-modules": "^0.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.1",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "axios": "^0.19.0",
    "babel-jest": "^24.8.0",
    "babel-polyfill": "^6.26.0",
    "chai": "4.2.0",
    "fast-xml-parser": "^3.12.16",
    "jest": "^24.8.0",
    "md5": "^2.2.1",
    "mocha-junit-reporter": "^1.23.1",
    "mocha-multi-reporters": "^1.1.7",
    "mysql2": "^1.6.5",
    "query-string": "^6.8.1",
    "socket.io-client": "^2.2.0"
  }
}
